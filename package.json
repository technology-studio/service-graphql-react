{
  "description": "Technology Studio - Service graphql react",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/technology-studio/service-graphql-react.git"
  },
  "author": {
    "name": "Technology Studio",
    "email": "npm@technologystudio.sk",
    "url": "https://bitbucket.org/technology-studio/"
  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "concurrently \"yarn workspaces run build:watch\" \"jest --watch\"",
    "build": "yarn wpd build && yarn wd build",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "compare-boilerplate-version": "./scripts/compare-boilerplate-version.sh",
    "lint": "eslint --format=node_modules/eslint-formatter-pretty --ext .jsx,.js,.ts,.tsx .",
    "fixcode": "eslint --format=node_modules/eslint-formatter-pretty --ext .jsx,.js,.ts,.tsx . --fix",
    "git-hook": "yarn -s lint-staged",
    "ncu-all": "yarn workspaces run ncu -i && ncu -i",
    "postinstall": "husky install",
    "print-txo-packages": "yarn list 2> /dev/null|grep @txo|sed 's/.*\\(@txo[^@]*\\)@^*\\([^ ]*\\).*/\\1@\\2/g'|sort|uniq",
    "sanity": "yarn lint && yarn build && tsc --noEmit && yarn test && yarn compare-boilerplate-version && yarn outdated && echo 'success'",
    "update-boilerplate-version": "./scripts/update-boilerplate-version.sh",
    "wd": "yarn workspace @txo/service-graphql-react",
    "wpd": "yarn workspace @txo-peer-dep/service-graphql-react"
  },
  "workspaces": [
    "packages/@txo/*",
    "packages/@txo-peer-dep/*"
  ],
  "dependencies": {},
  "devDependencies": {
    "@apollo/client": "^3.6.5",
    "@txo-peer-dep/log": "^4.0.0",
    "@txo-peer-dep/service-error-handler-react": "^1.2.0",
    "@txo-peer-dep/service-graphql": "^3.2.0",
    "@txo/hooks-react": "^2.3.2",
    "@txo/log-console": "^3.0.0",
    "@txo/service-graphql": "^4.3.0",
    "@txo/service-prop": "^2.2.0",
    "@txo/types": "^1.3.0",
    "@types/jest": "^27.5.1",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.set": "^4.3.7",
    "@types/react": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "concurrently": "^7.2.1",
    "eslint": "^7.32.0",
    "eslint-config-txo-typescript": "^2.3.1",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "graphql": "^15.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.2",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "npm-check-updates": "^13.0.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "redux-saga": "^1.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2"
  }
}
